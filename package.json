    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "typescript": "4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint:fix"
  }
